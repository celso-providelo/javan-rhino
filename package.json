    "Bartek Szopka <bartek.szopka@canonical.com>",
    "Katie Fenn <katie.fenn@canonical.com>",
    "Stephen Stewart <stephen.stewart@canonical.com>"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/canonical-ols/javan-rhino/issues"
  },
  "homepage": "https://github.com/canonical-ols/javan-rhino#readme",
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "concurrently": "^2.2.0",
    "coveralls": "^2.11.12",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.0",
    "eslint-plugin-react": "^6.1.0",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^3.0.2",
    "normalize.css": "^4.2.0",
    "nyc": "^8.1.0",
    "postcss-loader": "^0.10.0",
    "postcss-simple-vars": "^3.0.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.6.1",
    "style-loader": "^0.13.1",
