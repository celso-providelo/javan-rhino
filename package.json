{
  "name": "javan-rhino",
  "private": true,
  "version": "1.0.0",
  "description": "Front-end to Ubuntu Store Payments",
  "main": "index.js",
  "scripts": {
    "start-dev": "concurrently --kill-others \"npm run watch-client\" \"node server/\"",
    "watch-client": "node webpack/webpack-dev-server.js",
    "lint": "eslint -c .eslintrc.js '**/*.js'",
    "test": "npm run lint && nyc --require babel-core/register mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canonical-ols/javan-rhino.git"
  },
